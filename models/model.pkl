# gerar_modelo_fake.py
import joblib
from sklearn.linear_model import LinearRegression
import numpy as np

# Treino fake
X = np.array([[1], [2], [3]])
y = np.array([2, 4, 6])
modelo = LinearRegression().fit(X, y)

# Salvar no formato esperado
joblib.dump(modelo, "model/model.pkl")
print("Modelo fake salvo em model/model.pkl")